[html]
directory = coverage

[paths]
source =
    src/bigxml/
    .tox/py*/lib/python*/site-packages/bigxml/
    .tox/py*/site-packages/bigxml/

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    if __name__ == "__main__":
    @overload
    if TYPE_CHECKING:
show_missing = True

[run]
branch = True
source = bigxml
